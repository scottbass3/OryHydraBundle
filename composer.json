{
    "name": "scottbass3/ory-hydra-bundle",
    "description": "Symfony bundle for ory hydra client",
    "type": "symfony-bundle",
    "keywords": ["ory", "hydra", "api", "client"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Scottbass3\\OryHydraBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "scottbass",
            "email": "scottbass41@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "^6.0|^7.0",
        "scottbass3/hydra-client-php-psr-18": "^1.0",
        "symfony/http-client": "^6.0|^7.0",
        "guzzlehttp/promises": "^2.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.89"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
